{
  "name": "netlify-lm-plugin",
  "description": "Netlify CLI plugin to work with Netlify Large Media",
  "version": "0.1.0",
  "author": "David Calavera @calavera",
  "bugs": "https://github.com/netlify/netlify-lm-plugin/issues",
  "dependencies": {
    "@netlify/cli-utils": "^1.0.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "boxen": "^2.1.0",
    "chalk": "^2.4.2",
    "hasbin": "^1.2.3",
    "listr": "^0.14.3",
    "netlify": "^2.2.4",
    "node-fetch": "^2.3.0",
    "semver": "^5.6.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/listr": "^0.13.0",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^7",
    "tslint": "^5",
    "typescript": "^3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/netlify/netlify-lm-plugin",
  "keywords": [
    "netlify",
    "netlify-cli",
    "netlify-cli-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "netlify",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "netlify/netlify-lm-plugin",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
